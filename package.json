{
  "name": "@keratagpro/tagpro-analytics-bulk-download",
  "version": "0.0.1",
  "description": "Downloads matches in bulk from tagpro.eu. Converts the results to jsonlines and compresses them with gzip by default.",
  "author": "Kera",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "git://github.com/keratagpro/tagpro-analytics-bulk-download.git"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix && prettier --write .",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm run lint",
    "preversion": "npm run lint && changelog-updater --check",
    "version": "changelog-updater && git add CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "axios": "^1.2.3",
    "cheerio": "^1.0.0-rc.12",
    "event-stream": "^4.0.1",
    "jsonlines": "^0.1.1",
    "JSONStream": "^1.3.5"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/event-stream": "^4.0.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "changelog-updater": "^2.0.3",
    "debug": "^4.3.4",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "prettier": "^2.8.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
